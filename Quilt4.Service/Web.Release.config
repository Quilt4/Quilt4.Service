<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=xxxxxx;Initial Catalog=Quilt4;User ID=sa;Password=xxxxxx" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <Quilt4>
    <Quilt4Net ProjectApiKey="" Enabled="true" UseBuildTime="false" xdt:Transform="Replace">
      <Session Environment="Prod" />
      <Target Location="https://service.Quilt4.com/" Timeout="60" />
    </Quilt4Net>
  </Quilt4>

</configuration>