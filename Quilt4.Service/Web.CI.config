<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=yyyyyy;Initial Catalog=Quilt4CI;User ID=sa;Password=yyyyyy" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  
  <Quilt4>
    <Quilt4Net ProjectApiKey="" Enabled="true" UseBuildTime="false" xdt:Transform="Replace">
      <Session Environment="CI" />
      <Target Location="https://service.Quilt4.com/" Timeout="60" />
    </Quilt4Net>
  </Quilt4>

</configuration>